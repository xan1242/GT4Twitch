    <PackageReference Include="WebView2.Runtime.X64" Version="116.0.1938.69" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include=".\PDTools\PDTools.SaveFile\PDTools.SaveFile.csproj" />
  </ItemGroup>

